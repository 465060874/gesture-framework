<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.VBox?>
<fx:root fx:id="element" type="javafx.scene.layout.VBox"
         xmlns:fx="http://javafx.com/fxml"
         stylesheets="/css/Element.css"
         maxHeight="-Infinity"
        >
    <children>
        <Label alignment="CENTER" text="Simplify Contour" fx:id="label" minWidth="-Infinity"/>
        <VBox fx:id="configPane">
            <children>
                <Label text="Accuracy"/>
                <Slider orientation="HORIZONTAL" max="15" min="0" fx:id="accuracySlider"/>
            </children>
        </VBox>
    </children>
</fx:root>
