<?import javafx.scene.layout.HBox?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.shape.Polygon?>
<?import java.lang.*?>
<fx:root fx:id="elementLink" type="javafx.scene.layout.Pane"
         xmlns:fx="http://javafx.com/fxml"
         stylesheets="/css/ElementLink.css"
         HBox.hgrow="ALWAYS"
         prefWidth="-1.0" minHeight="50" minWidth="40" maxWidth="-1.0">

  <children>
    <Line startX="0" startY="25" endX="40" endY="25" strokeWidth="3"
            fx:id="line"/>
    <Polygon fx:id="triangle" fill="DODGERBLUE" stroke="BLACK" strokeType="INSIDE">
      <points>
        <Double fx:value="-6.0" />
        <Double fx:value="6.0" />
        <Double fx:value="-6.0" />
        <Double fx:value="-6.0" />
        <Double fx:value="6.0" />
        <Double fx:value="0.0" />
      </points>
    </Polygon>
  </children>
</fx:root>
