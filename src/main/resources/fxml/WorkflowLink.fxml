<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.Line?>
<?import javafx.scene.shape.Polygon?>
<?import java.lang.*?>
<fx:root fx:id="workflowLink" type="javafx.scene.layout.AnchorPane"
         xmlns:fx="http://javafx.com/fxml"
         stylesheets="/css/WorkflowLink.css"
         minHeight="50" minWidth="40">
         <!--maxHeight="-Infinity" maxWidth="-Infinity" -->
         <!--prefHeight="-Infinity" prefWidth="-Infinity" >-->
  <children>
    <Line startX="0" startY="-50" endX="40" endY="50" strokeWidth="3"
            fx:id="line"/>
    <Polygon fx:id="triangle" fill="DODGERBLUE" stroke="BLACK" strokeType="INSIDE">
      <points>
        <Double fx:value="-6.0" />
        <Double fx:value="6.0" />
        <Double fx:value="-6.0" />
        <Double fx:value="-6.0" />
        <Double fx:value="6.0" />
        <Double fx:value="0.0" />
      </points>
    </Polygon>
  </children>
</fx:root>
