<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<fx:root fx:id="workflowContainer" type="javafx.scene.layout.VBox"
         xmlns:fx="http://javafx.com/fxml"
         stylesheets="/css/WorkflowContainer.css" maxHeight="-Infinity">
  <children>
      <HBox fx:id="header">
          <children>
            <Label alignment="CENTER" text="WorkflowContainer" fx:id="containerLabel"
                    minWidth="-Infinity"/>
            <Button text="Add Workflow" fx:id="addButton" />
        </children>
      </HBox>

      <VBox fx:id="workflowsBox" maxHeight="-Infinity"/>

    </children>
</fx:root>
