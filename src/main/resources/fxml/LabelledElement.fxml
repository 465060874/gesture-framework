<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<fx:root fx:id="element" type="javafx.scene.layout.VBox"
         xmlns:fx="http://javafx.com/fxml"
         stylesheets="/css/Element.css"
         minHeight="50"
         minWidth="70"
         maxHeight="50"
         maxWidth="70"
         >
    <children>
        <Label alignment="CENTER" text="Element" fx:id="label" minWidth="-Infinity"/>
        <Button text="Click!" fx:id="clickButton"/>
        <Label text="no click data" fx:id="clicksLabel"/>
    </children>
</fx:root>
